{
    "name": 'nfr',
    "script": 'app.py',
    "interpreter": './venv/bin/python',
    "instances": 1,
    "autorestart": true,
    "watch": ['app.py'],
    "max_memory_restart": '1G',
    "log_date_format" : 'YYYY-DD-MM HH:mm:ss.SSS',
    "out_file": "stdout.log",
    "error_file": "stderr.log"
}